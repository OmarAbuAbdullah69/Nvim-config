			"sonokai",
			"tokyonight-night",
			"nightfly",
			"material-deep-ocean",
			"neofusion"
		]
	},
	"options":{
		"backup":false,
		"clipboard":"unnamedplus",
		"cmdheight":1,
		"completeopt":["menuone", "noselect"],
		"conceallevel":0,
		"fileencoding":"utf-8",
		"hlsearch":true,
		"ignorecase":true,
		"mouse":"a",
		"pumheight":10,
		"showmode":false,
		"showtabline":2,
		"smartcase":true,
		"smartindent":true,
		"splitbelow":true,
		"splitright":true,
		"swapfile":false,
		"timeoutlen":300,
		"undofile":true,
		"updatetime":300,
		"writebackup":false,
		"expandtab":false,
		"shiftwidth":2,
		"tabstop":2,
		"cursorline":true,
		"number":true,
		"relativenumber":true,
		"numberwidth":2,
		"signcolumn":"yes",
		"wrap":false,
		"linebreak":true,
		"scrolloff":8,
		"sidescrolloff":8,
		"guifont":"monospace:h17",
		"whichwrap":"bs<>[]hl"
	},
	"keymaps":{
		"leader":"<Space>",
		"modes":{
			"n":{
				"<leader>q":":q<CR>",
				"<leader>w":":w<CR>",
				"<leader>wq":":wq<CR>",
				"<leader>bj":":BufferNext<CR>",
				"<leader>bh":":BufferPrevious<CR>",
				"<leader>bc":":BufferClose<CR>",
				"dp":"d$",
				"<C-h>":"<C-w>h",
				"<C-j>":"<C-w>j",
				"<C-k>":"<C-w>k",
				"<C-l>":"<C-w>l",
				"<C-Up>":":resize -2<CR>",
				"<C-Down>":":resize +2<CR>",
				"<C-Left>":":vertical resize -2<CR>",
				"<A-Up>":":MoveLine -1<CR>",
				"<A-Down>":":MoveLine 1<CR>",
				"<A-S-Left>":":MoveWord -1<CR>",
				"<A-S-Right>":":MoveWord 1<CR>",
				"<C-Right>":":vertical resize +2<CR>",
				"<leader>e":":NvimTreeToggle<CR>",
				"<leader>ff":":Telescope fd<CR>",
				"<leader>f":":Telescope<CR>",
				"<leader>b+":":Boole increment<CR>",
				"<leader>b-":":Boole decrement<CR>"
			},
			"i":{

			},
			"v":{
				"<":"<gv^",
				">":">gv^",
				"<A-Left>":":MoveHBlock -1<CR>",
				"<A-Right>":":MoveHBlock 1<CR>"
			},
			"x":{
				"<A-Up>":":MoveBlock 1<CR>",
				"<A-Down>":":MoveBlock -1<CR>"
			}
		}
	},
	"filetree":{
		"sort":{
			"items_first":"folders"
		},
		"view":{
			"width":30,
			"side":"right",
			"centralize_selection":true,
			"cursorline":true
		},
		"modified":{
			"enable":true,
			"show_ondir":true
		},
		"filters":{
			"enable":true,
			"dotfiles":true
		},
		"diagnostics":{
			"enable":true,
			"show_ondir":true
		},
		"renderer":{
			"group_empty":false,
			"icons":{
				"file":true,
				"folder":true,
				"folder_arrow":true,
				"git":true
			}
		},
		"actions":{
			"close_on_fileopen":true
		}
	}
