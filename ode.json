{
	"completion":{
		"select":"<CR>",
		"abort":"q",
		"complete":"<C-Space>",
		"next":"<Tab>",
		"prev":"<S-Tab>"
	},
	"themes":{
		"available":{
			"monokai-pro":"loctvl842/monokai-pro.nvim",
			"wich-dark":"sontungexpt/witch",
			"oxocarbon":"nyoom-engineering/oxocarbon.nvim",
			"sonokai":"sainnhe/sonokai",
			"tokyonight-night":"folke/tokyonight.nvim",
			"nightfly":"bluz71/vim-nightfly-colors",
